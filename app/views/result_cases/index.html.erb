<h1>Listing result_cases</h1>

<table>
  <thead>
    <tr>
      <th>Rd</th>
      <th>Test case</th>
      <th>Result suite</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @result_cases.each do |result_case| %>
      <tr>
        <td><%= result_case.rd_id %></td>
        <td><%= result_case.test_case_id %></td>
        <td><%= result_case.result_suite_id %></td>
        <td><%= link_to 'Show', result_case %></td>
        <td><%= link_to 'Edit', edit_result_case_path(result_case) %></td>
        <td><%= link_to 'Destroy', result_case, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result case', new_result_case_path %>
